{
  "name": "discord-bot",
  "version": "1.0.0",
  "main": "dist/index.js",
  "type": "commonjs",
  "scripts": {
    "build": "tsc",
    "start": "tsc && node dist/src/app/index.js",
    "start:dev": "nodemon --exec ts-node /src/app/index.js",
    "lint": "eslint .",
    "worker": "tsc && pm2 start dist/src/app/index.js --no-daemon",
    "heroku-postbuild": "npm install pm2 --save",
    "db:deploy": "npx prisma migrate deploy && npx prisma generate"
  },
  "dependencies": {
    "@prisma/client": "^6.4.1",
    "@types/express": "^5.0.0",
    "bcryptjs": "^3.0.2",
    "discord-api-types": "^0.37.111",
    "discord.js": "^14.15.3",
    "dotenv": "^16.4.7",
    "express": "^4.19.2",
    "node-cron": "^3.0.3",
    "node-schedule": "^2.1.1",
    "nodemailer": "^6.9.16",
    "nodemon": "^3.1.9",
    "pm2": "^5.4.3",
    "prisma": "^6.4.1",
    "telegraf": "^4.16.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "eslint": "^9.17.0",
    "eslint-plugin-import": "^2.31.0",
    "globals": "^15.14.0",
    "prisma-dbml-generator": "^0.12.0",
    "typescript": "^5.7",
    "typescript-eslint": "^8.18.1"
  }
}
